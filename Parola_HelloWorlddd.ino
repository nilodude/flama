void setup(void)
{
  mxObj.begin();
  mxObj.clear();
  cols = mxObj.getColumnCount();
}

void loop(void) {
  
  for ( int j = 0; j < numFrames-1; j++ ) {

    for (int i = 0; i < cols; i++ ) {
      
        mxObj.setColumn(i, llamita[j][cols-1-i]);
      
    }
    delay(5);
  }  
  delay(5);
  for ( int j = numFrames-2; j >= 0; j-- ) {

    for (int i = 0; i < cols; i++ ) {
      
        mxObj.setColumn(i, llamita[j][cols-1-i]);
      
    }
    delay(5);
  }  
}
